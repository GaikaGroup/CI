{
  "name": "sveltekit-tutor-site",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src/lib/modules/auth/stores.js src/routes/login src/routes/signup",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "vitest run tests/auth",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:api": "vitest run tests/api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "vitest run tests/smoke",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:smoke && npm run test:e2e",
    "test:audit-api": "node scripts/audit-api-coverage.js",
    "db:test": "node src/lib/database/test-connection.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:backup": "bash scripts/backup-db.sh",
    "db:backup:pre-migration": "bash scripts/backup-before-migration.sh",
    "db:cleanup": "bash scripts/cleanup-db-connections.sh",
    "db:restore": "bash scripts/restore-db.sh",
    "clear-sessions": "node scripts/clear-sessions.js",
    "check-sessions": "node scripts/check-sessions.js",
    "create-test-users": "node scripts/create-test-users.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.32.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@sveltejs/adapter-node": "^1.3.1",
    "@tensorflow-models/face-landmarks-detection": "^1.0.6",
    "@tensorflow/tfjs": "^4.22.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "face-api.js": "^0.22.2",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.23",
    "lucide-svelte": "^0.300.0",
    "pdfjs-dist": "^5.3.93",
    "pg": "^8.16.3",
    "prisma": "^6.16.3",
    "tesseract.js": "^5.0.3"
  }
}
